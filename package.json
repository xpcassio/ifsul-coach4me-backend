  },
  "type": "commonjs",
  "dependencies": {
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "cors": "^2.8.5"
  }
}
